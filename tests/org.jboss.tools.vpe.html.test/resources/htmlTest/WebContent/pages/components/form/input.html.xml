<tests>
	<test id="checkbox">
		<input type="checkbox" id="checkbox" class="checkboxClass"
			checked="" style="color: red;" />
	</test>
	<test id="file">
		<input type="file" size="30"
			style="color: red;" value="value" />
	</test>
	<test id="input">
		<input id="input" value="input" class="inputClass"
			style="color: red;" />
	</test>
	<test id="button">
		<input type="button" id="button" value="input button" dir="ltr"
			class="buttonClass" style="color: red;" />

	</test>
	<test id="image">
		<input type="image" class="imageClass"
			src="/img/image.jpeg" style="color: red;" />
	</test>
	<test id="password">
		<input type="password" id="password" value="password" class="passwordClass"
			style="color: red;" dir="ltr" size="40" />
	</test>
	<test id="radio">
		<input type="radio" id="radio"
			style="color: red;" checked="" />
	</test>

	<test id="reset">
		<input type="reset" id="reset"
			style="color: red;" class="resetClass"/>
	</test>
	<test id="submit">
		<input type="submit" id="submit" value="submit" class="submitClass"
			dir="ltr" style="color: red;" />

	</test>
	<test id="text">
		<input type="text" value="value" id="text" class="textClass"
			style="color: red;" size="40" dir="ltr" />

	</test>
</tests>